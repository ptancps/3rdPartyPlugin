<style>
    .searchbar-section{
      display: flex;
      align-items: center;
      justify-content: space-between;
      background: white;
      box-shadow: 0px 0px 10px gray;
   }
   .searchbar-section .left-section{
     width: 100%;
     margin: 0em 2em;
   }
   .searchbar-section .left-section .search-bar{
      display: flex;
      align-items: center;
     width: 100%;
   }
   .searchbar-section .left-section .search-bar input{
     width: 100%;
     padding: 1em;
     outline: none;
     border: none;
     font-size: 18px;
   }

   .searchbar-section .search-bar svg{ 
      width: 25px;
      height: 25px;
   }

   .searchbar-section .right-section{
      display: flex;
      align-items: center;
      margin: 0em 1em;
      padding: 1em 0em;
   }
   .searchbar-section .right-section .notification svg{
      display: flex;
      align-items: center;
      width: 25px;
      height: 25px;
      margin: 0em 0.5em;
      cursor: pointer;
   }
   .searchbar-section .right-section .profile svg{
      display: flex;
      align-items: center;
      width: 30px;
      height: 30px;
      margin: 0em 0.5em;
      cursor: pointer;
   }
   .searchbar-section .left-section .left-inner-addon {
     position: relative;
   }

   .searchbar-section .left-section .left-inner-addon input {
     margin-left: 1em;
   }

   .searchbar-section .left-section .left-inner-addon i {
    position: absolute;
    left: 0;
    pointer-events: none;
   }
</style>
